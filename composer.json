{
  "name": "adexyme/openapi-to-postman",
  "description": "Converts OpenAPI specs., especially as generated by the Scramble API documentation generator, to a corresponding Postman collection in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adegoke Akintoye",
      "email": "adexyme@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Adexyme\\OpenApiToPostman\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adexyme\\OpenApiToPostman\\OpenApiToPostmanServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^10.0"
  }
}
